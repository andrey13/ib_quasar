{
    "name": "zakaz-quasar",
    "version": "0.0.1",
    "description": "A Quasar Project",
    "productName": "zakaz-quasar",
    "author": "omega-3 <andrey13@yandex.ru>",
    "private": true,
    "scripts": {
        "lint": "eslint --ext .js,.vue ./",
        "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
        "test": "echo \"No test specified\" && exit 0",
        "dev": "quasar dev",
        "srv": "nodemon backend/server.mjs",
        "dbf": "node backend/exportDbf.mjs",
        "srv-dev": "concurrently \"npm run srv\" \"quasar dev\"",
        "srv-dev1": "concurrently \"yarn run srv\" \"quasar dev\"",
        "qs": "node src/background.mjs",
        "build": "quasar build",
        "win": "quasar build -m electron -T win32 --arch x64",
        "inst": "npm run prepack:win64 && electron-builder --win --x64 --project dist/electron/UnPackaged/ --prepackaged …/[app-name]-win32-x64/",
        "prepack:win64": "quasar build -m electron -t win32 -a x64",
        "pack:win64": "npm run prepack:win64 && electron-builder --win --x64 --project dist/electron/UnPackaged/ --prepackaged …/[app-name]-win32-x64/",
        "b": "quasar build && electron-builder"
    },
    "main": "server.mjs",
    "dependencies": {
        "@quasar/extras": "^1.16.4",
        "aws-sdk": "^2.1417.0",
        "axios": "^1.2.1",
        "cors": "^2.8.5",
        "dbffile": "^1.9.2",
        "express": "^4.18.2",
        "mock-aws-s3": "^4.0.2",
        "nock": "^13.3.2",
        "pinia": "^2.0.11",
        "quasar": "^2.6.0",
        "sqlite3": "^5.1.6",
        "tabulator-tables": "^5.5.0",
        "vue": "^3.0.0",
        "vue-router": "^4.0.0"
    },
    "devDependencies": {
        "@pinegrow/vite-plugin": "^3.0.43",
        "@quasar/app-vite": "^1.3.0",
        "autoprefixer": "^10.4.2",
        "concurrently": "^8.2.0",
        "cross-env": "^7.0.3",
        "electron": "^25.3.0",
        "electron-builder": "^24.4.0",
        "electron-devtools-installer": "^3.2.0",
        "electron-packager": "^17.1.1",
        "eslint": "^8.10.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-vue": "^9.0.0",
        "nodemon": "^3.0.1",
        "postcss": "^8.4.14",
        "prettier": "^2.5.1"
    },
    "engines": {
        "node": "^18 || ^16 || ^14.19",
        "npm": ">= 6.13.4",
        "yarn": ">= 1.21.1"
    }
}
